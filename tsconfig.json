{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "target": "ES2022",
        "lib": ["ES2023", "dom", "DOM.Iterable"],
        "jsx": "react-jsx",
        "strict": true,
        "allowJs": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true
    },
    "ts-node": {
        "compilerOptions": {
            "module": "CommonJS"
        }
    }
}
